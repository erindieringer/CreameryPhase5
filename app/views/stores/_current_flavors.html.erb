 <%unless !@store.active %>
	<% unless @current_flavors.empty? %>
	  <div class="panel callout radius">
	    <h3>Current Flavors for <%= @store.name %> </h3>
	    <ul>
	      <% @current_flavors.each do |flavor| %>
	        <li><%= link_to flavor.flavor.name, flavor_path(flavor.flavor) %></li>
	      <% end %>
	    </ul>
	  </div>
	<% end %>
<%end%>